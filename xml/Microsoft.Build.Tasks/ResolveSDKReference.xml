          <source>Set of SDK References to resolve to paths on disk</source>
          <target state="translated">Набор ссылок на SDK, преобразуемых в пути на диске</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.ResolveSDKReference.SDKReferences">
          <source>Returns <ph id="ph1">&lt;see cref="T:Microsoft.Build.Framework.ITaskItem" /&gt;</ph>.</source>
          <target state="translated">Возвращает <ph id="ph1">&lt;see cref="T:Microsoft.Build.Framework.ITaskItem" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.ResolveSDKReference.TargetedSDKArchitecture">
