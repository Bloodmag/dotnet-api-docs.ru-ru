      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Tracking.ActivityScheduledRecord">
          <source>Represents a tracking record of an activity being scheduled for execution.</source>
          <target state="translated">Представляет запись отслеживания действия, которое запланировано для выполнения.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityScheduledRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>The instance ID of the scheduled activity.</source>
          <target state="translated">Идентификатор экземпляра запланированного действия.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityScheduledRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>A sequence that indicates the order in which tracking records are generated.</source>
          <target state="translated">Последовательность, которая указывает порядок создания записей отслеживания.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityScheduledRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>The activity issuing the scheduled execution request.</source>
          <target state="translated">Действие, которое выдает запрос на запланированное выполнение.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityScheduledRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>The scheduled activity.</source>
          <target state="translated">Запланированное действие.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityScheduledRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.Activities.Tracking.ActivityInfo)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityScheduledRecord" /&gt;</ph> class using the specified instance ID, record number, activity and child activity.</source>
          <target state="translated">Инициализирует новый экземпляр класса <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityScheduledRecord" /&gt;</ph>, используя указанный идентификатор экземпляра, номер записи, действие и дочернее действие.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.ActivityScheduledRecord.Activity">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> for the activity.</source>
          <target state="translated">Возвращает объект <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> для действия.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.ActivityScheduledRecord.Activity">
          <source>The activity information.</source>
          <target state="translated">Сведения о действии.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.ActivityScheduledRecord.Child">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> for the child activity</source>
          <target state="translated">Возвращает объект <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> для дочернего действия.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.ActivityScheduledRecord.Child">
          <source>The child activity information.</source>
          <target state="translated">Сведения о дочернем действии.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityScheduledRecord.ToString">
          <source>Returns a string that contains all of the details of the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityScheduledRecord" /&gt;</ph>.</source>
          <target state="translated">Возвращает строку, которая содержит все подробные сведения объекта <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityScheduledRecord" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityScheduledRecord.ToString">
          <source>The tracking record details.</source>
          <target state="translated">Подробные сведения о записи отслеживания.</target>       </trans-unit>
