        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.ActivityInstance">
          <source>Represents the instance state of an activity at runtime.</source>
          <target state="translated">Представляет состояние экземпляра действия во время выполнения.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Activities.ActivityInstance.Activity">
          <source>Gets the activity represented by the current <ph id="ph1">&lt;see cref="T:System.Activities.ActivityInstance" /&gt;</ph>.</source>
          <target state="translated">Возвращает действие, представляемое текущим экземпляром <ph id="ph1">&lt;see cref="T:System.Activities.ActivityInstance" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.ActivityInstance.Activity">
          <source>The activity represented by this <ph id="ph1">&lt;see cref="T:System.Activities.ActivityInstance" /&gt;</ph>.</source>
          <target state="translated">Действие, представляемое этим экземпляром <ph id="ph1">&lt;see cref="T:System.Activities.ActivityInstance" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.Activities.ActivityInstance.Id">
