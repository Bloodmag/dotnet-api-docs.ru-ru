      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Linq.Mapping.MetaAccessor">
          <source>Represents an accessor to a member.</source>
          <target state="translated">Представляет метод доступа к члену.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Data.Linq.Mapping.MetaAccessor" /&gt;</ph> class.</source>
          <target state="translated">Инициализирует новый экземпляр класса <ph id="ph1">&lt;see cref="T:System.Data.Linq.Mapping.MetaAccessor" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.GetBoxedValue(System.Object)">
          <source>The instance from which to get the value or on which to set the value.</source>
          <target state="translated">Экземпляр, для которого нужно задать или из которого нужно прочитать значение.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.GetBoxedValue(System.Object)">
          <source>Specifies an object on which to set a value or from which to get a value.</source>
          <target state="translated">Задает объект, для которого нужно задать или из которого нужно прочитать значение.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.GetBoxedValue(System.Object)">
          <source>The boxed value of this instance.</source>
          <target state="translated">Упакованное значение этого экземпляра.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasAssignedValue(System.Object)">
          <source>The instance being looked at.</source>
          <target state="translated">Проверяемый экземпляр.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasAssignedValue(System.Object)">
          <source>Specifies whether the instance has an assigned value.</source>
          <target state="translated">Указывает, присвоено ли данному экземпляру значение.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasAssignedValue(System.Object)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the instance currently has an assigned value; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">Значение <ph id="ph1">&lt;see langword="true" /&gt;</ph>, если экземпляру присвоено значение; в противном случае — значение <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasLoadedValue(System.Object)">
          <source>The instance being looked at.</source>
          <target state="translated">Проверяемый экземпляр.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasLoadedValue(System.Object)">
          <source>Specifies whether the instance has a value loaded from a deferred source.</source>
          <target state="translated">Указывает, содержит ли экземпляр значение, загруженное из отложенного источника.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasLoadedValue(System.Object)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the instance currently has a value loaded from a deferred source; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">Значение <ph id="ph1">&lt;see langword="true" /&gt;</ph>, если экземпляр содержит значение, загруженное из отложенного источника; в противном случае — значение <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasValue(System.Object)">
          <source>The instance being looked at.</source>
          <target state="translated">Проверяемый экземпляр.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasValue(System.Object)">
          <source>Specifies whether the instance has a loaded or assigned value.</source>
          <target state="translated">Указывает, какое значение содержит экземпляр: загруженное или присвоенное.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.HasValue(System.Object)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the instance currently has a loaded or assigned value; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">Значение <ph id="ph1">&lt;see langword="true" /&gt;</ph>, если экземпляр содержит загруженное или присвоенное значение; в противном случае — значение <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.SetBoxedValue(System.Object@,System.Object)">
          <source>The instance into which to set the value.</source>
          <target state="translated">Экземпляр, для которого задается значение.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.SetBoxedValue(System.Object@,System.Object)">
          <source>The value to set.</source>
          <target state="translated">Задаваемое значение.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.Data.Linq.Mapping.MetaAccessor.SetBoxedValue(System.Object@,System.Object)">
          <source>Sets the value as an object.</source>
          <target state="translated">Задает значение как объект.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="P:System.Data.Linq.Mapping.MetaAccessor.Type">
          <source>Gets the type of the member accessed by this accessor.</source>
          <target state="translated">Получает тип члена, которому принадлежит данный метод доступа.</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Linq.Mapping.MetaAccessor.Type">
          <source>The type of the member.</source>
          <target state="translated">Тип элемента.</target>       </trans-unit>
