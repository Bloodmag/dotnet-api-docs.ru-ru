          <target state="translated">Получает число записей журнала.</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.EnterpriseServices.CompensatingResourceManager.Clerk.LogRecordCount">
