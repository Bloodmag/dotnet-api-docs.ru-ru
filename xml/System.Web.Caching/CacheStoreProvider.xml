          <source>The key.</source>
          <target state="translated">Ключ.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Web.Caching.CacheStoreProvider.AddDependent(System.String,System.Web.Caching.CacheDependency,System.DateTime@)">
          <source>The dependant item.</source>
          <target state="translated">Зависимый элемент.</target>       </trans-unit>
