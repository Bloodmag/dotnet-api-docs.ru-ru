          <source>The key.</source>
          <target state="translated">Ключ.</target>       </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve" uid="M:System.Web.Caching.CacheStoreProvider.RemoveDependent(System.String,System.Web.Caching.CacheDependency)">
          <source>The dependency.</source>
          <target state="translated">Зависимость.</target>       </trans-unit>
