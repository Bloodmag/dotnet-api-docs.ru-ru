      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Data.BindingStatus">
          <source>Describes the status of a binding.</source>
          <target state="translated">Описывает состояние привязки.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Data.BindingStatus">
          <source>XAML Text Usage</source>
          <target state="translated">Использование текста XAML</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Data.BindingStatus">
          <source>You do not typically use this enumeration in <ph id="ph1">[!INCLUDE[TLA2#tla_xaml](~/includes/tla2sharptla-xaml-md.md)]</ph>.</source>
          <target state="translated">Обычно не используется в это перечисление <ph id="ph1">[!INCLUDE[TLA2#tla_xaml](~/includes/tla2sharptla-xaml-md.md)]</ph>.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.Active">
          <source>The binding has been successfully activated.</source>
          <target state="translated">Привязка успешно активирована.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.Active">
          <source>This means that the binding has been attached to its binding target (target) property and has located the binding source (source), resolved the <ph id="ph1">&lt;see langword="Path" /&gt;</ph> and/or <ph id="ph2">&lt;see langword="XPath" /&gt;</ph>, and begun transferring values.</source>
          <target state="translated">Это означает, что привязка была присоединена к свойству целевого объекта привязки (цели) и нашла источник привязки (источник), разрешила <ph id="ph1">&lt;see langword="Path" /&gt;</ph> и (или) <ph id="ph2">&lt;see langword="XPath" /&gt;</ph> и начала передачу значений.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.AsyncRequestPending">
          <source>The binding is waiting for an asynchronous operation to complete.</source>
          <target state="translated">Привязка ожидает завершения асинхронной операции.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.Detached">
          <source>The binding has been detached from its target property.</source>
          <target state="translated">Привязка была отсоединена от свойства целевого объекта.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.Inactive">
          <source>The binding has not been activated.</source>
          <target state="translated">Привязка не активирована.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.PathError">
          <source>The binding was unable to resolve the source path.</source>
          <target state="translated">Привязке не удалось разрешить путь источника.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.Unattached">
          <source>The binding has not yet been attached to its target property.</source>
          <target state="translated">Привязка еще не присоединена к своему целевому свойству.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.UpdateSourceError">
          <source>The binding was unable to send the value to the source property.</source>
          <target state="translated">Привязке не удалось отправить значение в исходное свойство.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.UpdateTargetError">
          <source>The binding could not successfully return a source value to update the target value.</source>
          <target state="translated">Привязке не удалось вернуть исходное значение для обновления целевого значения.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Windows.Data.BindingStatus.UpdateTargetError">
          <source>For more information, see <ph id="ph1">&lt;see cref="P:System.Windows.Data.BindingBase.FallbackValue" /&gt;</ph>.</source>
          <target state="translated">Дополнительные сведения см. в разделе <ph id="ph1">&lt;see cref="P:System.Windows.Data.BindingBase.FallbackValue" /&gt;</ph>.</target>       </trans-unit>
