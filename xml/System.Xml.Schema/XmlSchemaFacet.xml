      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Xml.Schema.XmlSchemaFacet">
          <source>Abstract class for all facets that are used when simple types are derived by restriction.</source>
          <target state="translated">Абстрактный класс для всех аспектов, которые используются при создании производных простых типов путем ограничения.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Xml.Schema.XmlSchemaFacet">
          <source>A facet is defined as an element.</source>
          <target state="translated">Аспект задается элементом.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Xml.Schema.XmlSchemaFacet">
          <source>Each <ph id="ph1">`facet`</ph> element has a <ph id="ph2">`fixed`</ph> attribute that is a Boolean value.</source>
          <target state="translated">Каждый <ph id="ph1">`facet`</ph> элемент имеет <ph id="ph2">`fixed`</ph> атрибут, который является логическим значением.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Xml.Schema.XmlSchemaFacet">
          <source>All the facets for a simple type together define the set of legal values for that simple type.</source>
          <target state="translated">Все аспекты для простого типа совместно определяют набор допустимых значений для простого типа.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Xml.Schema.XmlSchemaFacet">
          <source>See <ph id="ph1">&lt;xref:System.Xml.Schema.XmlSchemaEnumerationFacet&gt;</ph> for an example.</source>
          <target state="translated">Пример см. в разделе <ph id="ph1">&lt;xref:System.Xml.Schema.XmlSchemaEnumerationFacet&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Xml.Schema.XmlSchemaFacet.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Xml.Schema.XmlSchemaFacet" /&gt;</ph> class.</source>
          <target state="translated">Инициализирует новый экземпляр класса <ph id="ph1">&lt;see cref="T:System.Xml.Schema.XmlSchemaFacet" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Xml.Schema.XmlSchemaFacet.IsFixed">
          <source>Gets or sets information that indicates that this facet is fixed.</source>
          <target state="translated">Получает или задает сведения, указывающие, что данный аспект зафиксирован.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Xml.Schema.XmlSchemaFacet.IsFixed">
          <source>If <ph id="ph1">&lt;see langword="true" /&gt;</ph>, value is fixed; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> — значение зафиксировано; в противном случае <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Xml.Schema.XmlSchemaFacet.IsFixed">
          <source>The default is <ph id="ph1">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">Значение по умолчанию: <ph id="ph1">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Xml.Schema.XmlSchemaFacet.IsFixed">
          <source>Optional.</source>
          <target state="translated">Необязательный.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Xml.Schema.XmlSchemaFacet.IsFixed">
          <source>This property is unused by <ph id="ph1">`XmlSchemaEnumerationFacet`</ph> and <ph id="ph2">`XmlSchemaPatternFacet`</ph> because these classes cannot be fixed.</source>
          <target state="translated">Это свойство не используется с <ph id="ph1">`XmlSchemaEnumerationFacet`</ph> и <ph id="ph2">`XmlSchemaPatternFacet`</ph> из-за этих классов не может быть устранена.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Xml.Schema.XmlSchemaFacet.Value">
          <source>Gets or sets the <ph id="ph1">&lt;see langword="value" /&gt;</ph> attribute of the facet.</source>
          <target state="translated">Получает или задает атрибут <ph id="ph1">&lt;see langword="value" /&gt;</ph> для аспекта.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Xml.Schema.XmlSchemaFacet.Value">
          <source>The value attribute.</source>
          <target state="translated">Атрибут значения.</target>       </trans-unit>
